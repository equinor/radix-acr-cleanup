    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/acr-cleanup/main.go",
            "env": {},
            "args": [
                "--period=10s",
                "--registry=radixdev",
                "--clusterType=development",
                "--deleteUntagged=false",
                "--performDelete=false",
                "--cleanupDays=su,mo,tu,we,th,fr,sa",
                "--cleanupStart=0:00",
                "--cleanupEnd=6:00"
            ]
        }
    ]
